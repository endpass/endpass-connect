{
  "name": "endpass-connect",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/lib/endpass-connect.js",
  "module": "./dist/lib/endpass-connect.umd.js",
  "scripts": {
    "build": "rimraf ./dist && npm run build:app && npm run build:lib",
    "dev:app": "vue-cli-service serve ./packages/app/src/main.js",
    "build:app": "vue-cli-service --no-clean build ./packages/app/src/main.js",
    "dev:lib": "./node_modules/rollup/bin/rollup -c -w",
    "build:lib": "./node_modules/rollup/bin/rollup -c --silent",
    "test:unit": "vue-cli-service test:unit"
  },
  "lint-staged": {
    "./dist/lib/**/*.js": [
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build:lib && lint-staged"
    }
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "reset.css": "^2.0.2",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-unit-jest": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "postcss-nested": "^4.1.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.4",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "vue-template-compiler": "^2.5.17"
  }
}

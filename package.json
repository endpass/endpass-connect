{
  "name": "@endpass/connect",
  "version": "0.19.0-beta",
  "authVersion": "",
  "main": "./dist/endpass-connect.min.js",
  "module": "./dist/endpass-connect.esm.js",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endpass/endpass-connect.git"
  },
  "files": [
    "src",
    "dist"
  ],
  "author": "Endpass, Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/endpass/endpass-connect/issues"
  },
  "homepage": "https://github.com/endpass/endpass-connect",
  "scripts": {
    "dev": "rollup -c -w",
    "build:dev": "rollup -c",
    "build": "rimraf ./dist && npm run build:lib && npm run build:browser",
    "build:lib": "NODE_ENV=production rollup -c",
    "build:browser": "webpack --mode production",
    "test": "jest",
    "unit:watch": "jest --watch",
    "format": "prettier --write './src/**/*.{js,json}' && eslint ./src/**/*.{js} --fix",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "check-types": "tsc"
  },
  "lint-staged": {
    "./src/**/*.{js,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "release": {
    "branch": "release",
    "prepare": [
      {
        "path": "@semantic-release/changelog"
      },
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ]
        }
      ]
    ]
  },
  "publishConfig": {
    "tag": "latest"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run check-types",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@endpass/class": "^0.12.1",
    "lodash.get": "^4.4.2",
    "utility-types": "^3.4.1",
    "web3-providers-http": "1.0.0-beta.33"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/parser": "^7.4.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.2",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "@types/lodash": "^4.14.122",
    "@types/lodash.get": "^4.4.6",
    "@types/node": "^11.11.6",
    "@types/web3": "^1.0.18",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-multiple-parsers": "^1.0.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "regenerator-runtime": "^0.13.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.7.3",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.1",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-visualizer": "^1.1.0",
    "semantic-release": "^15.13.3",
    "typescript": "^3.3.3333",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  }
}

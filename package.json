{
  "name": "@endpass/connect",
  "version": "0.29.4-beta",
  "authVersion": "",
  "main": "./endpass-connect.min.js",
  "module": "./endpass-connect.esm.js",
  "umd": "./endpass-connect.umd.js",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endpass/endpass-connect.git"
  },
  "author": "Endpass, Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/endpass/endpass-connect/issues"
  },
  "homepage": "https://github.com/endpass/endpass-connect",
  "scripts": {
    "update": "yarn build && npm publish ./dist",
    "dev": "rollup -c -w",
    "dev:browser": "webpack --mode development -w",
    "build": "rimraf ./dist && npm run build:lib && npm run build:browser",
    "build:lib": "NODE_ENV=production rollup -c",
    "build:browser:dev": "rollup -c & webpack --mode development",
    "build:browser": "webpack --mode production",
    "build:dev": "NODE_ENV=development rollup -c",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest ./tests",
    "test:unit:watch": "jest ./tests --watch",
    "test:e2e:cy:open": "cypress open",
    "test:e2e": "node ./scripts/e2e-run.js",
    "test:e2e:open": "node ./scripts/e2e-open.js",
    "test:e2e:dev": "node ./scripts/e2e-dev.js",
    "test:e2e:setup": "node ./scripts/e2e-setup.js",
    "test:e2e:serve": "node ./scripts/e2e-serve.js",
    "format": "yarn format:prettier && yarn format:eslint",
    "format:prettier": "prettier --write '**/*.{js,json}'",
    "format:eslint": "eslint --ext .json,.js,.ts src/ --fix",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "check-types": "tsc"
  },
  "lint-staged": {
    "./src/**/*.{js,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "release": {
    "branch": "release",
    "prepare": [
      {
        "path": "@semantic-release/changelog"
      },
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ]
        }
      ]
    ]
  },
  "publishConfig": {
    "tag": "latest"
  },
  "config": {
    "repo": "https://github.com/endpass/endpass-auth.git",
    "commit": "HEAD",
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@endpass/class": "^0.19.2",
    "@endpass/utils": "^1.8.7",
    "axios": "^0.19.0",
    "axios-token-interceptor": "^0.1.0",
    "callbag-basics": "^3.1.0",
    "callbag-concat-map": "^1.1.0",
    "callbag-from-promise": "^1.3.0",
    "callbag-map-promise": "^0.1.3",
    "callbag-subscribe": "^1.5.1",
    "callbag-switch-map": "^1.0.2",
    "callbag-take-while": "^2.0.0",
    "callbag-tap": "^1.3.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "utility-types": "^3.9.0",
    "web3-providers-http": "1.0.0-beta.33"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/parser": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime": "^7.6.3",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@endpass/eslint-plugin-endpass": "^0.2.9",
    "@semantic-release/changelog": "^3.0.5",
    "@semantic-release/git": "^7.0.18",
    "@types/axios-token-interceptor": "^0.1.2",
    "@types/lodash": "^4.14.144",
    "@types/lodash.get": "^4.4.6",
    "@types/node": "^12.12.5",
    "@types/web3": "^1.0.20",
    "axios-mock-adapter": "^1.17.0",
    "babel-loader": "^8.0.6",
    "commitizen": "^4.0.3",
    "concurrently": "^5.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cypress": "^3.4.1",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.6.0",
    "fs-extra": "^8.1.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-localstorage-mock": "^2.4.0",
    "lint-staged": "^9.4.2",
    "mkdirp": "^0.5.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.18.2",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.0",
    "rollup": "^1.21.4",
    "rollup-plugin-alias": "^1.5.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-visualizer": "^2.7.2",
    "semantic-release": "^15.13.30",
    "serve": "^11.2.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}

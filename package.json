{
  "name": "@endpass/connect",
  "version": "0.27.0-beta",
  "authVersion": "",
  "main": "./endpass-connect.min.js",
  "module": "./endpass-connect.esm.js",
  "umd": "./endpass-connect.umd.js",
  "connectPlugins": {
    "provider": {
      "main": "./provider.min.js",
      "module": "./provider.js",
      "umd": "./provider.umd.js",
      "browser": "./provider.browser.js"
    }
  },
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endpass/endpass-connect.git"
  },
  "files": [
    "src",
    "dist"
  ],
  "author": "Endpass, Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/endpass/endpass-connect/issues"
  },
  "homepage": "https://github.com/endpass/endpass-connect",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rimraf ./dist && npm run build:lib && npm run build:browser",
    "build:lib": "NODE_ENV=production rollup -c",
    "build:browser:dev": "rollup -c & webpack --mode development",
    "build:browser": "webpack --mode production",
    "build:dev": "rollup -c",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest ./tests",
    "test:unit:watch": "jest ./tests --watch",
    "test:e2e:cy:open": "cypress open",
    "test:e2e": "node ./scripts/e2e-run.js",
    "test:e2e:open": "node ./scripts/e2e-open.js",
    "test:e2e:dev": "node ./scripts/e2e-dev.js",
    "test:e2e:setup": "node ./scripts/e2e-setup.js",
    "test:e2e:serve": "node ./scripts/e2e-serve.js",
    "format": "prettier --write './src/**/*.{js,json}' && eslint ./src/**/*.{js} --fix",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "check-types": "tsc"
  },
  "lint-staged": {
    "./src/**/*.{js,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "release": {
    "branch": "release",
    "prepare": [
      {
        "path": "@semantic-release/changelog"
      },
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ]
        }
      ]
    ]
  },
  "publishConfig": {
    "tag": "latest"
  },
  "config": {
    "repo": "https://github.com/endpass/endpass-auth.git",
    "commit": "HEAD",
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run check-types",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@endpass/class": "^0.17.3",
    "@endpass/utils": "^1.8.2",
    "axios": "^0.19.0",
    "axios-token-interceptor": "^0.1.0",
    "callbag-basics": "^3.1.0",
    "callbag-concat-map": "^1.1.0",
    "callbag-from-promise": "^1.3.0",
    "callbag-map-promise": "^0.1.3",
    "callbag-subscribe": "^1.5.1",
    "callbag-switch-map": "^1.0.2",
    "callbag-take-while": "^2.0.0",
    "callbag-tap": "^1.3.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "utility-types": "^3.7.0",
    "web3-providers-http": "1.0.0-beta.33"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/parser": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.5.5",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@types/lodash": "^4.14.136",
    "@types/lodash.get": "^4.4.6",
    "@types/node": "^12.6.8",
    "@types/web3": "^1.0.19",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "axios-mock-adapter": "^1.17.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "commitizen": "^4.0.3",
    "copy-webpack-plugin": "^5.0.4",
    "cypress": "^3.4.1",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-multiple-parsers": "^1.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "fs-extra": "^8.1.0",
    "gh-pages": "^2.0.1",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "jest-localstorage-mock": "^2.4.0",
    "lint-staged": "^9.2.1",
    "mkdirp": "^0.5.1",
    "prettier": "^1.18.2",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^2.6.3",
    "rollup": "^1.17.0",
    "rollup-plugin-alias": "^1.5.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-visualizer": "^2.5.4",
    "semantic-release": "^15.13.18",
    "serve": "^11.1.0",
    "typescript": "^3.5.3",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6"
  }
}

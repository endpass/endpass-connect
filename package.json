{
  "name": "endpass-connect",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/lib/endpass-connect.common.js",
  "module": "./dist/lib/endpass-connect.umd.min.js",
  "scripts": {
    "dev": "vue-cli-service serve ./packages/app/src/main.js",
    "build": "rimraf ./dist && npm run build:app && npm run build:lib && npm run dist",
    "build:app": "vue-cli-service --no-clean build ./packages/app/src/main.js",
    "build:lib": "vue-cli-service --no-clean build --target lib --name endpass-connect ./packages/lib/index.js",
    "test:unit": "vue-cli-service test:unit",
    "dist": "node ./scripts/dist.js",
    "postinstall": "node ./scripts/build.js"
  },
  "lint-staged": {
    "./packages/**/*.js": [
      "git add"
    ]
  },
  "husky": {
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "reset.css": "^2.0.2",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-unit-jest": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "postcss-nested": "^4.1.0",
    "rimraf": "^2.6.2",
    "vue-template-compiler": "^2.5.17"
  }
}
